# From the root, create a .env file and add in the following variables
OPENAI_API_KEY=your_api_key_here
MONGO_URI=Your Mongo URI e.g mongodb://localhost:27017/article-summarizer
PORT=5000
NODE_ENV=development or production
FRONTEND_URL=http://localhost:5173 or one of your choice
CLOUDINARY_URL=Your Cloudinary URL e.g cloudinary://xxxx:xxxxx-cloud-Name
USE_CLOUDINARY=true